{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/server.js"
    },
    {
      "source": "/health",
      "destination": "/api/server.js"
    },
    {
      "source": "/(.*)",
      "destination": "/public/$1"
    }
  ]
}
